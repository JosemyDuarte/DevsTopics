{
  "name": "tech-trending-topics",
  "version": "1.0.0",
  "description": "Tech news right where you want them",
  "main": "handler.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/jest --ci --verbose",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fixlint": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "buildtest": "tsc --noEmit",
    "deploy-qa": "npm run fixlint && ./node_modules/.bin/sls deploy --stage qa",
    "remove-qa": "./node_modules/.bin/sls remove --stage qa",
    "deploy-prod": "npm run fixlint && ./node_modules/.bin/sls deploy --stage prod",
    "remove-prod": "./node_modules/.bin/sls remove --stage prod",
    "build": "./node_modules/.bin/sls package",
    "dynamodb": "./node_modules/.bin/sls dynamodb start",
    "dynamodb-admin": "export DYNAMO_ENDPOINT=http://localhost:8000; ./node_modules/.bin/dynamodb-admin"
  },
  "dependencies": {
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.627.0",
    "node-fetch": "^2.6.0",
    "rss-parser": "^3.9.0",
    "source-map-support": "^0.5.10",
    "telegraf": "^3.38.0",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.10.5",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.5.7",
    "@types/twit": "^2.2.25",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-jest": "^25.1.0",
    "dynamodb-admin": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-module-resolver": "^0.16.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^25.1.0",
    "serverless": "^1.76.1",
    "serverless-create-global-dynamodb-table": "^2.1.0",
    "serverless-dynamodb-local": "0.2.37",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-offline": "^6.5.0",
    "serverless-offline-sns": "^0.68.0",
    "serverless-prune-plugin": "^1.4.2",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "Josemy Duarte",
  "license": "MIT"
}
